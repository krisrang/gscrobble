{
  "name": "gscrobble",
  "private": true,
  "engines": {
    "node": ">=0.8.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "del": "^2.2.0",
    "eslint-plugin-react": "^6.7.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cache": "^0.4.3",
    "gulp-chrome-manifest": "0.0.13",
    "gulp-clean-css": "^2.0.3",
    "gulp-eslint": "^3.0.1",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.0",
    "gulp-image-resize": "^0.10.0",
    "gulp-imagemin": "^3.1.1",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^1.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.2.0",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.2.1",
    "gulp-uglify": "^2.0.0",
    "gulp-useref": "^3.0.8",
    "gulp-zip": "^3.2.0",
    "main-bower-files": "^2.11.1",
    "run-sequence": "^1.1.5",
    "vinyl-source-stream": "^1.1.0",
    "wiredep": "^4.0.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true
    },
    "globals": {
      "chrome": true,
      "$": true,
      "React": true,
      "ReactDOM": true,
      "Raven": true
    },
    "rules": {
      "eol-last": 0,
      "react/no-set-state": 0,
      "react/no-multi-comp": 0,
      "react/jsx-max-props-per-line": 0,
      "no-console": 0,
      "quotes": [
        2,
        "single"
      ],
      "react/jsx-indent": [
        2,
        2
      ],
      "react/jsx-indent-props": [
        2,
        2
      ]
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/all"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true,
        "jsx": true
      }
    }
  },
  "dependencies": {}
}
